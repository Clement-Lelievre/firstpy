import random
max=random.randint(1,50)
secret_number=random.randint(1,max)
print('Hi! I am thinking of a number between 1 and', max, ' can you guess it?')
a=int(input())
while a != secret_number:
  if a>secret_number:
    print('too high')
    a=int(input())
  else:
    print('too low')
    a=int(input())
print('well done!')    
